#create a project
CREATE DATABASE eleve SET 'utf8';  --creer la data base eleves

CERATE TABLE IF NOT eleveAtagwa (id SMALLINT UNSIGNED NOT NULL AUTO_INCREMENT,
    prenom VARCHAR(40) NOT NULL,
    nom VARCHAR(30),
    date_naissance DATETIME NOT NULL,
    sexe CHAR(1),
    classe VARCHAR(10),
    tutorName VARCHAR(30),
    tutorPrename VARCHAR(30),
    tutorNumberPhone UNSIGNED BIGINT,  -- PEUTETRE AJOUTER UN DEUXIEME ET TROISIEME TUTOR
    PRIMARY KEY (id)
)
ENGINE=INNODB;                        -- creation de table eleves


SHOW TABLE;                           -- listes les tables de la base eleve


DESCRIBE eleveAtagwa;                 -- Listes les elements de la table eleveAtagwa
